function f(x, cc) { 
  let arr = Object.values(cc);
  
  for (let i =0; i< arr.length; i++) {
    if (arr[i]=== x) return arr[i+1] || arr[0]
  }
}

function f(x, cc) { 
  switch (x) {
    case cc.a: return cc.b;
    case cc.b: return cc.c;
    case cc.c: return cc.a;
  }
}

function f(x, cc) {
  return cc.a === x ? cc.b : cc.b === x ? cc.c : cc.a
}

