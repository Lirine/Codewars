//Задание 1. Дан массив чисел arr. Необходимо вернуть новый массив, в котором будут содержаться только повторяющиеся в исходном массиве элементы (дубликаты).

const arr = [-1, 2, 0, 2, 7, 7, 7, -1, 0, 8, 3, 4];
const duplicates = arr.filter(el => arr.indexOf(el) !== arr.lastIndexOf(el));
console.log(duplicates); // [-1, 2, 0, 2, 7, 7, 7, -1, 0]
