function switcher(x){
  let abc = ' zyxwvutsrqponmlkjihgfedcba!? ';
  let str = '';
  for (let i = 0; i < x.length; i++) {
    x[i] === abc[x[i]];
    str = str + abc[x[i]];
  }
  return str;
}

function switcher(x){
  let abc = ' zyxwvutsrqponmlkjihgfedcba!? ';
  return x.map((el,i) => el = abc[el]).join('');
}
